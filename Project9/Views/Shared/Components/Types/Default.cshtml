
@{
}

@model IEnumerable<string>


<div class="">
    <a asp-controller="Home" asp-action="Index" asp-route-Category="" asp-route-pageNum="1" class="btn btn-outline-secondary">Home</a>


    @foreach (string t in Model)
        {
            <a asp-controller="Home" asp-action="Index" asp-route-Category="@t" asp-route-pageNum="1" class="btn @(ViewBag.SelectedType == t ? "btn-outline-danger" :  "btn-outline-secondary")">@t</a>
        }
</div>